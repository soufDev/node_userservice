{
  "passPerPreset": true,
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "8"
        }
      }
    ]
  ],
  "plugins": [
    "syntax-async-functions",
    "transform-regenerator",
    "transform-runtime",
    "add-module-exports",
    "transform-es2015-destructuring",
    "transform-es2015-spread",
    "transform-object-rest-spread"
  ]
}
